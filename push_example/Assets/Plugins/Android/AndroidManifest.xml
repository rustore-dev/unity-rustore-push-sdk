      <meta-data android:name="unityplayer.UnityActivity" android:value="true" />
    </activity>
    <service android:name="ru.rustore.unitysdk.pushclient.RuStoreUnityMessagingService" android:exported="true" tools:ignore="ExportedService">
      <intent-filter>
        <action android:name="ru.rustore.sdk.pushclient.MESSAGING_EVENT" />
      </intent-filter>
    </service>
    <meta-data
        android:name="ru.rustore.sdk.pushclient.project_id"
        android:value="-Yv4b5cM2yfXm0bZyY6Rk7AHX8SrHmLI" />
    <meta-data
        android:name="ru.rustore.sdk.pushclient.params_class"
        android:value="ru.rustore.unitysdk.pushclient.RuStoreUnityPushClientParams" />
  </application>
</manifest>